<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="item_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/px_12_dp</item>
        <item name="android:layout_marginBottom">@dimen/px_12_dp</item>
        <item name="android:clipChildren">false</item>
        <item name="android:clipToPadding">false</item>
    </style>

    <style name="sub_item_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/px_12_dp</item>
        <item name="android:layout_marginBottom">@dimen/px_18_dp</item>
        <item name="android:layout_marginRight">@dimen/px_15_dp</item>
    </style>

    <style name="item_type_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="item_weight_type_style" parent="item_fitxy_type_style">
        <item name="android:layout_weight">1</item>
    </style>

    <style name="item_fitxy_type_style" parent="item_type_style">
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/ic_default</item>
    </style>

