<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/sub_type6_image_one"
        style="@style/item_779_312_style"
        android:layout_marginRight="@dimen/px_30_dp"
        android:layout_weight="1" />

    <LinearLayout
        style="@style/item_type_style"
        android:orientation="horizontal">

        <LinearLayout
            style="@style/item_type_style"
            android:layout_marginRight="@dimen/px_10_dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/sub_type6_image_two"
                style="@style/item_375_141_style"
                android:layout_marginBottom="@dimen/px_15_dp" />

            <ImageView
                android:id="@+id/sub_type6_image_three"
                style="@style/item_375_141_style"
                android:layout_marginTop="@dimen/px_15_dp" />
        </LinearLayout>

        <LinearLayout
            style="@style/item_type_style"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/sub_type6_image_four"
                style="@style/item_375_141_style"
                android:layout_marginBottom="@dimen/px_15_dp" />

            <ImageView
                android:id="@+id/sub_type6_image_five"
                style="@style/item_375_141_style"
                android:layout_marginTop="@dimen/px_15_dp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
